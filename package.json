    "electron": "^2.0.7",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-class-property": "^1.1.0",
