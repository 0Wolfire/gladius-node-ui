    "uglifyjs-webpack-plugin": "1.2.5",
    "webpack": "3.9.1",
    "webpack-dev-server": "2.9.5"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "test": "NODE_ENV=test MOCK_DATA=true nyc --require babel-register --all tape src/testUtils/setup.js 'src/**/*.test.js' | tap-nyc",
    "test:single": "./build/test-single.sh",
    "livereload": "livereload dist -d",
    "local": "NODE_ENV=development npm-run-all -p js:watch style:watch livereload",
    "local-mocked": "MOCK_DATA=true npm run local",
    "js:watch": "webpack-dev-server",
    "js:build": "webpack",
    "dev-build": "npm run clean && NODE_ENV=development npm-run-all -p js:build style:build",
    "build": "npm run clean && NODE_ENV=production npm-run-all -p js:build style:build",
    "style:build": "node-sass-chokidar src/ -o dist/ --include-path src/styles",
    "style:watch": "npm run style:build && node-sass-chokidar src/ -o dist/ --watch --recursive  --include-path src/styles",
    "lint": "eslint .",
    "eclint": "eclint check --block_comment_start '/*' --block_comment '*' --block_comment_end '*/'",
    "verify": "npm-run-all -s clean lint eclint test",
    "start": "npm run build && electron .",
    "package-mac": "electron-packager . Gladius --platform=darwin --arch=x64 --icon=\"img/icon/AppIcon.icns\" --ignore=\"build\" --out=\"build/macOS\" --overwrite",
    "package-win": "flatten-packages; npm install electron-packager; electron-packager . gladius-electron --platform=\"win32\" --arch=\"x64\" --ignore=\"build\" --out=\"build/Windows\" --overwrite",
    "package-linux": "electron-packager . Gladius --platform=\"linux\" --arch=\"x64\" --ignore=\"build\" --out=\"build/Linux\" --overwrite",
    "package": "npm run build; npm run package-win; npm run package-mac;npm run package-linux"
  },
